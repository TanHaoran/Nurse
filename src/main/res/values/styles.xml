<resources>

    <style name="AppTheme.Nurse" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/primary</item>
        <item name="colorPrimaryDark">@color/primary_dark</item>
        <item name="colorAccent">@color/accent</item>

        <item name="colorControlNormal">@color/iron</item>
        <item name="colorControlActivated">@color/white</item>
        <item name="colorControlHighlight">@color/white</item>
        <item name="android:textColorHint">@color/iron</item>

        <item name="colorButtonNormal">@color/primary</item>

    </style>

    <!--加载框-->
    <style name="AppTheme.Dark.Dialog" parent="Theme.AppCompat.Dialog">
        <item name="colorAccent">@color/white</item>
        <item name="android:textColorPrimary">@color/iron</item>
        <item name="android:background">@color/primary</item>
    </style>

    <!--个人中心行-->
    <style name="PersonalInfoLineStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">
            @dimen/personal_info_line_height
        </item>
        <item name="android:clickable">true</item>
        <item name="android:background">@drawable/plugin_line_selector
        </item>
        <item name="android:paddingLeft">
            @dimen/personal_info_line_paddingLeft
        </item>
    </style>

    <!--个人中心行左侧文字-->
    <style name="PersonalInfoLineLeftTextStyle">
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/normal_textColor</item>
        <item name="android:textSize">@dimen/normal_textSize</item>
    </style>

    <!--分割线-->
    <style name="DividerLineStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/divider_line_height
        </item>
        <item name="android:background">@color/divider_line</item>
    </style>

    <!--边框线-->
    <style name="DividerBorderStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">@color/iron</item>
    </style>

    <!--插件功能行-->
    <style name="PluginLineStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/plugin_line_height</item>
        <item name="android:background">@drawable/plugin_line_selector
        </item>
        <item name="android:clickable">true</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:paddingLeft">@dimen/line_paddingLeft
        </item>
    </style>

    <!--个人业务行-->
    <style name="BusinessLineStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/business_info_height
        </item>
        <item name="android:background">@drawable/plugin_line_selector
        </item>
        <item name="android:clickable">true</item>
        <item name="android:paddingLeft">@dimen/line_paddingLeft</item>
    </style>

    <!--进入箭头样式-->
    <style name="ArrowEnterStyle">
        <item name="android:layout_marginRight">@dimen/line_paddingRight
        </item>
        <item name="android:src">@drawable/arrow_right</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_centerVertical">true</item>
    </style>

    <style name="business_line_right_text_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:textColor">@color/monsoon</item>
        <item name="android:textSize">@dimen/personal_info_right_textSize
        </item>
    </style>

    <style name="personal_info_line_right_text_style">
        <item name="android:textColor">@color/gray_textColor</item>
        <item name="android:textSize">@dimen/normal_textSize</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginRight">@dimen/line_paddingRight
        </item>
    </style>

    <style name="PlugImageStyle">
        <item name="android:layout_width">@dimen/mine_plug_image_width
        </item>
        <item name="android:layout_height">@dimen/mine_plug_image_width
        </item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginLeft">
            @dimen/mine_plug_image_marginLeft
        </item>
    </style>

    <style name="Third_Party_login_Layout_Style">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:orientation">vertical</item>
    </style>

    <style name="Certificate_Line_Edit_Style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_marginLeft">
            @dimen/certificate_edit_marginLeft
        </item>
        <item name="android:background">@drawable/edit_no_border</item>
        <item name="android:maxLines">1</item>
        <item name="android:singleLine">true</item>
        <item name="android:paddingLeft">@dimen/line_paddingLeft</item>
        <item name="android:paddingRight">@dimen/line_paddingLeft</item>
        <item name="android:textColor">@color/gray_textColor</item>
        <item name="android:textCursorDrawable">@null</item>
        <item name="android:textSize">@dimen/normal_textSize</item>
        <item name="android:gravity">center_vertical|right</item>
    </style>

    <style name="Plug_Text_Style">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">@dimen/line_paddingLeft
        </item>
        <item name="android:layout_weight">1</item>
        <item name="android:textColor">@color/normal_textColor</item>
        <item name="android:textSize">@dimen/normal_textSize</item>
    </style>

    <style name="AudioDialog">
        <item name="android:windowBackground">@color/transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:backgroundDimEnabled">false</item>
    </style>

    <style name="Contact_Info_Line">
        <item name="android:paddingLeft">@dimen/line_paddingLeft</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_height">@dimen/personal_info_line_height</item>
        <item name="android:layout_width">match_parent</item>
    </style>

    <style name="Contact_Info_Line_Left_Text">
        <item name="android:layout_marginRight">@dimen/contact_detail_line_text_marginRight</item>
        <item name="android:textSize">@dimen/normal_textSize</item>
        <item name="android:textColor">@color/gray_textColor</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="Contact_Info_Line_Right_Text">
        <item name="android:textSize">@dimen/normal_textSize</item>
        <item name="android:textColor">@color/normal_textColor</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="SelectView_Style">
        <item name="android:layout_width">@dimen/select_view_width</item>
        <item name="android:layout_height">@dimen/select_view_width</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginLeft">@dimen/select_view_marginLeft</item>
        <item name="android:layout_marginRight">@dimen/select_view_marginRight</item>
    </style>

    <style name="tab_menu_text">
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">48dp</item>
        <item name="android:layout_centerInParent">true</item>
        <item name="android:textColor">@drawable/tab_menu_text</item>
    </style>

    <style name="tab_menu_bg_num">
        <item name="android:layout_width">20dp</item>
        <item name="android:layout_height">20dp</item>
        <item name="android:background">@drawable/bg_num</item>
        <item name="android:layout_marginLeft">-10dp</item>
        <item name="android:textSize">12sp</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/white</item>
    </style>



</resources>
